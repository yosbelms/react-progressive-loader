    "@types/react-dom": "^18.2.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "react",
    "reactjs",
    "img",
    "lazy load",
    "progressive",
    "medium",
    "loadable"
